---
title: Resulting From | Business, Technology, and Design
---
<div class="grid">
	<div class="col-3-12">
		<%= partial "sections/aside" %>
	</div>
	<div class="col-9-12">
		<section class="main-section">
			<% blog.articles[0...10].each do |article| %>
				<article class="post">
					<header class="post-header">
						<h1 class="post-title"><a href="<%= article.url %>"><%= article.title %></a></h1>
						<h2 class="post-date"><i class="fa fa-clock-o"></i> <%= article.date.strftime('%b %e %Y') %></h2>
						<h2 class="post-tags"><i class="fa fa-tags"></i> <span>Tagged:</span> <%= article.data.tags %></h2>
						<%= article.summary %>
					</header>
				</article>
			<% end %>
		</section>
	</div>
</div>